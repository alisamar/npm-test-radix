{
  "name": "npm-test-radix",
  "description": "test npm package",
  "version": "0.0.1",
  "files": [
    "dist/**/*",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.js",
      "import": "./dist/utils.mjs",
      "default": "./dist/utils.d.mts"
    },
    "./dist/*": "./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alisamar/npm-test-radix.git"
  },
  "bugs": {
    "url": "https://github.com/alisamar/npm-test-radix/issues"
  },
  "homepage": "https://github.com/alisamar/npm-test-radix#readme",
  "scripts": {
    "build": "pnpm clean && pnpm build:tsc && pnpm build:css",
    "build:tsc": "tsup src/index.ts src/utils.ts",
    "build:css": "node ./scripts/build.js",
    "clean": "rm -f -R tsconfig.tsbuildinfo dist",
    "prepack": "pnpm build",
    "release": "standard-version",
    "prepare": "husky",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.6",
    "babel-jest": "^29.7.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "jest": {
    "collectCoverage": true
  }
}
